{
  "name":"MyAdBlock",
  "description":"広告をへらす",
  "version":"1.0.0",
  "manifest_version":3,
  
  "permissions": [
    "storage",
    "activeTab"
  ],
  
  "action": {
    "default_popup": "popup.html",
    "default_title": "MyAdBlock"
  },
  
  "content_scripts":[
    {
      "matches":["*://*/*"],
      "js":["script.js"],
      "run_at":"document_end",
      "all_frames":false
    }
  ]
}